{
    "version": 2,
    "builds": [
        {
            "src": "./index.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "headers": {
        "/*": [
          "Access-Control-Allow-Origin: https://organize-kappa.vercel.app",
          "Access-Control-Allow-Credentials: true"
        ]
      }
}
